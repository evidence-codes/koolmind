{
  "expo": {
    "name": "koolmind",
    "slug": "koolmind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Vector.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/koolmind.png",
      "resizeMode": "contain",
      "backgroundColor": "#4577FF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Vector.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.evidence_codes.koolmind"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e1f5c444-a8ab-46e0-a5c8-ed29413a6402"
      }
    }
  }
}
